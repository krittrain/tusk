var peopleFactory = function(name,age,type){
 var temp ={};
 temp.age = age;
 temp.name = name;
 temp.type =type;
 temp.printPerson = function(){
 
console.log(this.name + "," + this.age + ","+ this.type);
 }
return temp;
};

var person1 = new peopleFactory('23','kritika','a');
var person2 = new peopleFactory('24','kritika1','b');

person1.printPerson();
person2.printPerson();


